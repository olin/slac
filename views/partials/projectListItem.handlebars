<div class="project-card">
    <div class="cover-container">
        <img class="project-cover" src="{{coverPhoto}}">
        <div class="project-title">{{title}}</div>
    </div>
    <div class="project-info">
        {{#each members}}
            <img class="member-icon" src="http://{{profilePhoto}}"/>
        {{/each}}
    </div>
</div>